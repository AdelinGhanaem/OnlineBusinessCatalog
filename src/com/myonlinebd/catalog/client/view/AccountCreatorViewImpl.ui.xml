<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:com.google.gwt.user.client.ui'
             xmlns:e='urn:import:com.google.gwt.editor.ui.client'>


  <b:HTMLPanel>

    <div>

      email:
      <e:ValueBoxEditorDecorator ui:field="email">
        <b:valuebox>
          <b:TextBox/>
        </b:valuebox>
      </e:ValueBoxEditorDecorator>
      password:
      <b:PasswordTextBox ui:field="password"/>
      <b:Button ui:field="submit"/>
    </div>

  </b:HTMLPanel>


</ui:UiBinder>